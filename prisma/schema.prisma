generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model cards {
  id                Int       @id @default(autoincrement())
  prompt            String
  answer            String
  understood        Int?      @default(0)
  nextscheduledtime DateTime?
}
